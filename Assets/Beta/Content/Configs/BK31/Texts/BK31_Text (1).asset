    \u0437\u0430\u0432\u0438\u0441\u0435\u043B \u043E\u0442 \u0441\u043D\u0430\u0431\u0436\u0435\u043D\u0438\u044F
    \u043F\u043E \u0440\u0435\u043A\u0435. \u041F\u043E\u044D\u0442\u043E\u043C\u0443
    \u0434\u043B\u044F \u043E\u0431\u0435\u0441\u043F\u0435\u0447\u0435\u043D\u0438\u044F
