    \u0441 23 \u0438\u044E\u043B\u044F 1942 \u043F\u043E 2 \u0444\u0435\u0432\u0440\u0430\u043B\u044F
    1943 \u0433\u043E\u0434\u0430. \u041E\u0441\u043E\u0431\u043E\u0435 \u043C\u0435\u0441\u0442\u043E
    \u0432 \u0421\u0442\u0430\u043B\u0438\u043D\u0433\u0440\u0430\u0434\u0441\u043A\u043E\u0439
    \u0431\u0438\u0442\u0432\u0435 \u0437\u0430\u043D\u0438\u043C\u0430\u0435\u0442
