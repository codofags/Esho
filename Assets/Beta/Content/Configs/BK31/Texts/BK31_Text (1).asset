    \u041E\u0442\u0435\u0447\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u0439
    \u0438 \u0412\u0442\u043E\u0440\u043E\u0439 \u041C\u0438\u0440\u043E\u0432\u043E\u0439
    \u0432\u043E\u0439\u043D\u044B, \u043F\u0440\u043E\u0445\u043E\u0434\u0438\u0432\u0448\u0430\u044F
    \u043A\u0430\u043A \u043A\u043E\u043C\u043F\u043B\u0435\u043A\u0441 \u043C\u043D\u043E\u0436\u0435\u0441\u0442\u0432\u0430
