    \u041D\u0430 \u043C\u0430\u0442\u0435\u0440\u0438\u0430\u043B\u0430\u0445 \u0411\u041A-31
    \u0431\u044B\u043B\u0430 \u0441\u043E\u0437\u0434\u0430\u043D\u0430 \u043F\u0435\u0440\u0435\u0434\u0432\u0438\u0436\u043D\u0430\u044F
