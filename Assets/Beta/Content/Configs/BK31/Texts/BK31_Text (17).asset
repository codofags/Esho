    \u0432\u044B\u0441\u0442\u0430\u0432\u043A\u0430, \u043A\u043E\u0442\u043E\u0440\u0430\u044F
    \u043F\u043E\u0431\u044B\u0432\u0430\u043B\u0430 \u0432 26 \u0433\u043E\u0440\u043E\u0434\u0430\u0445
    \u0420\u043E\u0441\u0441\u0438\u0438, \u043E\u0442 \u041C\u0443\u0440\u043C\u0430\u043D\u0441\u043A\u0430
    \u0434\u043E \u0412\u043B\u0430\u0434\u0438\u0432\u043E\u0441\u0442\u043E\u043A\u0430.
    \u0412\u044B\u0441\u0442\u0430\u0432\u043A\u0443 \u043F\u043E\u0441\u0435\u0442\u0438\u043B\u043E
    150 \u0442\u044B\u0441 \u0447\u0435\u043B\u043E\u0432\u0435\u043A. \u0421\u043E\u0432\u043C\u0435\u0441\u0442\u043D\u043E\u0441\u0442\u0438
    \u0441 \u0424\u041F\u0413 \u0440\u0435\u0430\u043B\u0438\u0437\u043E\u0432\u0430\u043D
    \u043F\u0440\u043E\u0435\u043A\u0442 \xAB\u0411\u041A-31. \u041F\u043E\u0438\u0441\u043A\xBB
    \u0412 \u0440\u0430\u043C\u043A\u0430\u0445 \u044D\u0442\u043E\u0433\u043E \u0432\u044B\u0441\u0442\u0430\u0432\u043E\u0447\u043D\u043E\u0433\u043E
    \u043F\u0440\u043E\u0435\u043A\u0442\u0430 \u0431\u044B\u043B\u0430 \u0440\u0435\u0430\u043B\u0438\u0437\u043E\u0432\u0430\u043D\u0430
    \u0434\u0435\u0442\u0441\u043A\u0430\u044F \u043E\u0431\u0440\u0430\u0437\u043E\u0432\u0430\u0442\u0435\u043B\u044C\u043D\u0430\u044F
    \u043B\u0430\u0431\u043E\u0440\u0430\u0442\u043E\u0440\u0438\u044F. \u0421\u043F\u0435\u0446\u0438\u0430\u043B\u0438\u0441\u0442\u044B,
    \u0443\u0447\u0430\u0441\u0442\u0432\u0443\u044E\u0449\u0438\u0435 \u0432 \u043F\u0440\u043E\u0435\u043A\u0442\u0435
    \u0440\u0430\u0441\u0441\u043A\u0430\u0437\u044B\u0432\u0430\u043B\u0438 \u043E
    \u0434\u0435\u0442\u044F\u043C \u043E \u0441\u0432\u043E\u0435\u0439 \u043F\u0440\u043E\u0444\u0435\u0441\u0441\u0438\u0438
    \u0438 \u0443\u043D\u0438\u043A\u0430\u043B\u044C\u043D\u043E\u043C, \u043D\u0435
    \u0432\u0441\u0435\u0433\u0434\u0430 \u0441\u043F\u0435\u0446\u0438\u0444\u0438\u0447\u0435\u0441\u043A\u043E\u043C
    \u043E\u043F\u044B\u0442\u0435, \u043F\u043E\u043B\u0443\u0447\u0435\u043D\u043D\u043E\u043C
