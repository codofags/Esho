    \u0413\u043E\u043B\u043E\u0434\u043D\u044B\u0439 9 \u043E\u043A\u0442\u044F\u0431\u0440\u044F
    1942 \u0433\u043E\u0434\u0430: \xAB... \u041E\u0442\u0442\u0443\u0434\u0430 \u044F
    \u043F\u0440\u043E\u0448\u0435\u043B \u043A \u0412\u043E\u043B\u0433\u0435. \u0422\u0430\u043C
    \u0441\u0442\u043E\u044F\u043B\u0438 \u0434\u0432\u0430 \u0437\u0430\u0445\u0432\u0430\u0447\u0435\u043D\u043D\u044B\u0445
    \u0440\u0443\u0441\u0441\u043A\u0438\u0445 76,2-\u043C\u043C \u043E\u0440\u0443\u0434\u0438\u044F,
