    \u0443\u0447\u0430\u0441\u0442\u043D\u0438\u043A \u0440\u043E\u043A\u043E\u0432\u044B\u0445
    \u0434\u043B\u044F \u0411\u041A-31 \u0438 \u0435\u0433\u043E \u044D\u043A\u0438\u043F\u0430\u0436\u0430
    \u0441\u043E\u0431\u044B\u0442\u0438\u0439 \u2013 \u043E\u0431\u0435\u0440-\u043B\u0435\u0439\u0442\u0435\u043D\u0430\u043D\u0442
    71-\u0439 \u043F\u0435\u0445\u043E\u0442\u043D\u043E\u0439 \u0434\u0438\u0432\u0438\u0437\u0438\u0438
    \u0432\u0435\u0440\u043C\u0430\u0445\u0442\u0430 \u0412\u0438\u0433\u0430\u043D\u0442
    \u0412\u044E\u0441\u0442\u0435\u0440. \u0412 \u0435\u0433\u043E \u043A\u043D\u0438\u0433\u0435
    \xAB\u0412 \u0430\u0434\u0443 \u0421\u0442\u0430\u043B\u0438\u043D\u0433\u0440\u0430\u0434\u0430.
    \u041A\u0440\u043E\u0432\u0430\u0432\u044B\u0439 \u043A\u043E\u0448\u043C\u0430\u0440
