    \u041D\u0430 \u044D\u0442\u043E\u0442 \u0440\u0430\u0437 \u043C\u044B \u043F\u0440\u0438\u0441\u0442\u0440\u0435\u043B\u044F\u043B\u0438\u0441\u044C
    \u043F\u043E \u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043D\u043E\u0439
    \u0442\u043E\u0447\u043A\u0435, \u0447\u0435\u0440\u0435\u0437 \u043A\u043E\u0442\u043E\u0440\u0443\u044E
    \u0432\u0441\u0435\u0433\u0434\u0430 \u043F\u0440\u043E\u0445\u043E\u0434\u0438\u043B
    \xAB\u043C\u043E\u043D\u0438\u0442\u043E\u0440\xBB. \u0412 \u044D\u0442\u043E\u0442
    \u0434\u0435\u043D\u044C \u043B\u043E\u0434\u043A\u0430 \u0434\u043E\u0448\u043B\u0430
